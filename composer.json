{
  "name": "furality/socket.io-php-emitter",
  "description": "PHP socket.io event emitter",
  "version":"0.1.3",
  "authors": [
    {
      "name": "Jace Ju",
      "email": "jaceju@gmail.com"
    },
    {
      "name": "Soma Szélpál",
      "email": "szelpalsoma@gmail.com"
    },
    {
      "name": "Anton Pavlov",
      "email": "anton.pavlov.it@gmail.com"
    },
    {
      "name": "Kakious",
      "email": "kakious@kakio.us"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "predis/predis": "^1.1",
    "rybakit/msgpack": "^0.9.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1.5",
    "phpunit/php-code-coverage": "^9.2",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Furality\\SocketIO\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cover-html": "XDEBUG_MODE=coverage phpunit --coverage-html coverage",
    "test-verbose": "phpunit --verbose"
  },
  "license": "MIT"
}
